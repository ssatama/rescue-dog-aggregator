{
  "name": "rescue-dog-aggregator-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "node scripts/pre-build-health-check.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "check:duplicates": "node scripts/fix-duplicates.js",
    "fix:duplicates": "node scripts/fix-duplicates.js --interactive",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "e2e:ui": "playwright test --ui",
    "e2e:install": "playwright install",
    "e2e:validate": "npx tsx e2e-tests/utils/testIdValidatorCli.ts",
    "e2e:validate:strict": "npx tsx e2e-tests/utils/testIdValidatorCli.ts --strict",
    "e2e:validate:build": "npx tsx e2e-tests/utils/testIdValidatorCli.ts --strict",
    "e2e:strict": "E2E_SELECTOR_STRATEGY=strict playwright test",
    "e2e:docs": "playwright test test-id-validation.spec.ts --grep \"Required Test IDs\"",
    "e2e:mobile": "playwright test --project mobile",
    "e2e:mobile:headed": "playwright test --project mobile --headed",
    "e2e:mobile:debug": "playwright test --project mobile --debug",
    "e2e:mobile:report": "playwright show-report mobile-test-results",
    "e2e:touch": "playwright test mobile-*-interactions.spec.ts",
    "e2e:responsive": "playwright test responsive-*.spec.ts",
    "e2e:accessibility:mobile": "playwright test touch-accessibility-validation.spec.ts",
    "e2e:devices": "playwright test --project \"iPhone 15 Pro - Mobile Safari\" --project \"Samsung Galaxy S21 - Mobile Chrome\" --project \"iPad Mini 6th Gen - Mobile Safari\"",
    "e2e:mobile:install": "playwright install chromium webkit",
    "test:e2e": "playwright test",
    "test:e2e:critical": "playwright test --grep @critical",
    "test:e2e:fast": "playwright test --grep @critical --reporter=line",
    "test:e2e:full": "playwright test --reporter=html",
    "test:e2e:optimized": "playwright test --config=playwright.config.optimized.ts",
    "test:e2e:optimized:critical": "playwright test --config=playwright.config.optimized.ts --grep @critical",
    "test:e2e:optimized:critical:ci": "playwright test --config=playwright.config.ci.ts --grep @critical --grep-invert \"Home page loads and displays core content|Dogs page loads and displays dogs|Search functionality works\"",
    "test:e2e:optimized:critical:ci:debug": "playwright test --config=playwright.config.ci.ts --grep @critical",
    "test:e2e:failure-report": "playwright test --config=playwright.config.optimized.ts --reporter=./e2e-tests/reporters/failure-reporter.ts",
    "test:e2e:quick-debug": "playwright test --config=playwright.config.optimized.ts --grep @critical --max-failures=5",
    "test:e2e:full:quick-debug": "playwright test --config=playwright.config.ts --max-failures=5"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@playwright/test": "^1.54.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.0",
    "@sentry/nextjs": "^10.30.0",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "aria-hidden": "^1.2.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "focus-trap-react": "^11.0.5",
    "framer-motion": "^12.24.12",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1",
    "next-mdx-remote": "^5.0.0",
    "node-fetch": "^3.3.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.3",
    "react-intersection-observer": "^10.0.0",
    "react-simple-maps": "^3.0.0",
    "react-swipeable": "^7.0.2",
    "react-virtualized-auto-sizer": "^2.0.2",
    "react-window": "^2.2.4",
    "react-window-infinite-loader": "^2.0.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.1.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@chromatic-com/storybook": "^4.0.1",
    "@next/bundle-analyzer": "^16.1.1",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/nextjs-vite": "^10.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-simple-maps": "^3.0.6",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/browser": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^30.2.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.28.0",
    "eslint-config-next": "^16.1.1",
    "eslint-plugin-storybook": "^10.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "playwright": "^1.54.1",
    "postcss": "^8.4.23",
    "prettier": "^3.7.4",
    "puppeteer": "^24.15.0",
    "storybook": "^10.1.11",
    "tailwindcss": "^4.1.18",
    "ts-unused-exports": "^11.0.1",
    "typescript": "^5.9.3",
    "unimport": "^5.6.0",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "d3-color": "^3.1.0"
  }
}
